{
  "name": "ngx-toastr",
  "description": "Toastr for Angular",
  "homepage": "https://github.com/scttcper/ngx-toastr",
  "bugs": "https://github.com/scttcper/ngx-toastr/issues",
  "repository": "https://github.com/scttcper/ngx-toastr.git",
  "version": "8.8.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "rimraf dist && ts-node build.ts",
    "publish": "cd dist && npm publish",
    "bundlesize": "npx bundlesize",
    "test": "ng test --browsers=ChromeCI",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeCI",
    "lint": "ng lint ngx-toastr --type-check ---format stylish",
    "lint:fix": "ng lint ngx-toastr --type-check --fix ---format stylish",
    "ghpages": "ng build --prod --no-progress --baseHref='/ngx-toastr/'",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.4",
    "@angular/common": "6.0.4",
    "@angular/compiler": "6.0.4",
    "@angular/core": "6.0.4",
    "@angular/forms": "6.0.4",
    "@angular/http": "6.0.4",
    "@angular/platform-browser": "6.0.4",
    "@angular/platform-browser-dynamic": "6.0.4",
    "@angular/router": "6.0.4",
    "bootstrap": "4.1.1",
    "core-js": "2.5.7",
    "lodash-es": "4.17.10",
    "rxjs": "6.1.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.4",
    "@angular/language-service": "6.0.4",
    "@types/fs-extra": "5.0.3",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash-es": "4.17.0",
    "@types/node": "10.1.4",
    "@types/rimraf": "2.0.2",
    "codelyzer": "4.3.0",
    "fs-extra": "6.0.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-packagr": "3.0.1",
    "protractor": "5.3.2",
    "rimraf": "2.6.2",
    "ts-loader": "4.3.0",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.7.2"
  },
  "bundlesize": [
    {
      "path": "./dist/bundles/ngx-toastr.umd.min.js",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/esm2015/ngx-toastr.js",
      "maxSize": "8 kB"
    },
    {
      "path": "./dist/esm5/ngx-toastr.js",
      "maxSize": "8 kB"
    },
    {
      "path": "./dist/toastr.css",
      "maxSize": "2 kB"
    },
    {
      "path": "./dist/toastr-old.css",
      "maxSize": "3 kB"
    },
    {
      "path": "./dist/toastr-bs4-alert.scss",
      "maxSize": "2 kB"
    }
  ]
}
