{
  "name": "ngx-toastr",
  "description": "Toastr for Angular",
  "homepage": "https://github.com/scttcper/ngx-toastr",
  "bugs": "https://github.com/scttcper/ngx-toastr/issues",
  "repository": "scttcper/ngx-toastr",
  "version": "0.0.0-placeholder",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ts-node -O '{\"module\": \"commonjs\"}' build.ts",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "test:ci": "ng test --watch=false --code-coverage --no-progress",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ghpages": "ng build --prod --no-progress --baseHref='/ngx-toastr/'",
    "e2e": "ng e2e",
    "bundlesize": "npx bundlesize",
    "travis-deploy-once": "travis-deploy-once",
    "commit": "git-cz",
    "semantic-release": "cd dist && semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.2",
    "@angular/common": "6.1.2",
    "@angular/compiler": "6.1.2",
    "@angular/core": "6.1.2",
    "@angular/forms": "6.1.2",
    "@angular/http": "6.1.2",
    "@angular/platform-browser": "6.1.2",
    "@angular/platform-browser-dynamic": "6.1.2",
    "@angular/router": "6.1.2",
    "@ctrl/ngx-github-buttons": "2.0.4",
    "bootstrap": "4.1.3",
    "core-js": "2.5.7",
    "lodash-es": "4.17.10",
    "rxjs": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.3",
    "@angular/cli": "6.1.3",
    "@angular/compiler-cli": "6.1.2",
    "@angular/language-service": "6.1.2",
    "@types/fs-extra": "5.0.4",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash-es": "4.17.1",
    "@types/node": "10.7.0",
    "@types/rimraf": "2.0.2",
    "codelyzer": "4.4.4",
    "fs-extra": "7.0.0",
    "jasmine-core": "3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "ng-packagr": "4.1.0",
    "protractor": "5.4.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.9.8",
    "travis-deploy-once": "5.0.2",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "2.9.2"
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "bundlesize": [
    {
      "path": "./dist/bundles/ngx-toastr.umd.min.js",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/esm2015/ngx-toastr.js",
      "maxSize": "8 kB"
    },
    {
      "path": "./dist/esm5/ngx-toastr.js",
      "maxSize": "8 kB"
    },
    {
      "path": "./dist/toastr.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/toastr-old.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/toastr-bs4-alert.scss",
      "maxSize": "5 kB"
    }
  ]
}
