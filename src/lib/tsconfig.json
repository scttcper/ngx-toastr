{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "../../deploy",
    "sourceMap": false,
    "target": "es5",
    "types": []
  },
  "angularCompilerOptions": {
    "genDir": "../../waste"
  },
  "exclude": [
    "node_modules"
  ]
}
